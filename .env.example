APP_NAME=Solar
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE="UTC"
APP_URL=https://solar-dev.test

# Enable for a simple log of SQL commands. See app/Providers/AppServiceProvider.php will only work when APP_ENV is 'local' or 'testing'
# queries are logged to `storage/logs/laravel-yyyy-mm-dd.log` as DEBUG logs.
PERF_PROFILE=false

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=daily
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Performance feature flags (opt-in)
FEATURE_CACHE_FORECAST_CHART=false
FORECAST_CHART_TTL=60
FEATURE_CACHE_STRAT_SUMMARY=false
STRAT_SUMMARY_TTL=600
# Optional downsampling toggles (default OFF)
FORECAST_DOWNSAMPLE=false
FORECAST_BUCKET_MINUTES=30
INVERTER_DOWNSAMPLE=false
INVERTER_BUCKET_MINUTES=30

MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# Sign up for Solis API: https://docs.solcast.com.au/
SOLCAST_API_KEY=
SOLCAST_RESOURCE_ID=

# Solcast allowance policy configuration
# Combined daily cap across forecast + actual requests
SOLCAST_DAILY_CAP=6
# ISO-8601 durations for minimum intervals and 429 backoff
SOLCAST_FORECAST_MIN_INTERVAL=PT4H
SOLCAST_ACTUAL_MIN_INTERVAL=PT8H
SOLCAST_429_BACKOFF=PT8H
# Reset window timezone (IANA TZ)
SOLCAST_RESET_TZ=UTC

# Existing customers can generate a key: https://octopus.energy/dashboard/new/accounts/personal-details/api-access
OCTOPUS_API_KEY=
OCTOPUS_ACCOUNT=
OCTOPUS_EXPORT_MPAN=
OCTOPUS_IMPORT_MPAN=
OCTOPUS_EXPORT_SERIAL_NUMBER=
OCTOPUS_IMPORT_SERIAL_NUMBER=

# Solis API access (optional)  Solis customers and sign up for support and request an API for
# use: https://solis-service.solisinverters.com
# to find SOLIS_INVERTER_ID fill in SOLIS_KEY_ID and SOLIS_KEY_SECRET then run `php artisan solis:inverter-list`
# Copy the `inverterId` number
SOLIS_KEY_ID=
SOLIS_KEY_SECRET=
SOLIS_INVERTER_ID=
SOLIS_API_URL=https://www.soliscloud.com:13333
